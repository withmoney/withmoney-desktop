{
  "coverageDirectory": "<rootDir>/coverage",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/coverage/",
    "package.json",
    "yarn.json",
    "coverage-final.json",
    "test-framework-setup.js",
    "jest.config.json"
  ],
  "setupFiles": [
    "<rootDir>/tests/test-framework-setup.js"
  ],
  "rootDir": "../",
  "testMatch": [
    "<rootDir>/tests/**/?(*.)test.{js,jsx}"
  ]
}

