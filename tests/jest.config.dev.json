{
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/coverage/",
    "package.json",
    "yarn.json",
    "coverage-final.json",
    "test-framework-setup.js",
    "jest.config.dev.json",
    "jest.config.json"
  ],
  "moduleFileExtensions": ["js"],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/styleMock.js",
    "\\.(css|scss)$": "<rootDir>/tests/styleMock.js",
    "^app(.*)$": "<rootDir>/src$1",
    "^api(.*)$": "<rootDir>/src/api$1",
    "^components(.*)$": "<rootDir>/src/components$1",
    "^pages(.*)$": "<rootDir>/src/pages$1",
    "^store(.*)$": "<rootDir>/src/store/modules$1",
    "^styles(.*)$": "<rootDir>/src/assets$1"
  },
  "snapshotSerializers": ["enzyme-to-json/serializer"],
  "setupFiles": ["<rootDir>/tests/test-framework-setup.js"],
  "rootDir": "../"
}
